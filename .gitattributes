# Track large JSON files with Git LFS
*.json filter=lfs diff=lfs merge=lfs -text
# Track CSV files with Git LFS (if they become large)
acts_data.csv filter=lfs diff=lfs merge=lfs -text
filtered_act_list.csv filter=lfs diff=lfs merge=lfs -text
# Track any large data files
Data/*.json filter=lfs diff=lfs merge=lfs -text
Data/*.csv filter=lfs diff=lfs merge=lfs -text
# Track compressed archives if any
*.zip filter=lfs diff=lfs merge=lfs -text
*.tar.gz filter=lfs diff=lfs merge=lfs -text
*.7z filter=lfs diff=lfs merge=lfs -text
# Track database files if any
*.db filter=lfs diff=lfs merge=lfs -text
*.sqlite filter=lfs diff=lfs merge=lfs -text
*.sqlite3 filter=lfs diff=lfs merge=lfs -text
*.csv filter=lfs diff=lfs merge=lfs -text
